--
-- Tabellenstruktur f¸r Tabelle `teams`
--

CREATE TABLE `teams` (
  `teamid` int(3) NOT NULL auto_increment,
  `teamname` varchar(255) NOT NULL default '',
  `teamcode` int(5) NOT NULL default '0',
  `teamgroup` int(5) default NULL,
  `club` varchar(255) NOT NULL default '',
  `spielgemeinschaft` int(1) NOT NULL default '0',
  `trainingDay1_tag` varchar(20) NOT NULL default '',
  `trainingDay1_halle` varchar(100) NOT NULL default '',
  `trainingDay1_fromHour` char(2) NOT NULL default '',
  `trainingDay1_fromMinutes` char(2) NOT NULL default '',
  `trainingDay1_toHour` char(2) NOT NULL default '',
  `trainingDay1_toMinutes` char(2) NOT NULL default '',
  `trainingDay2_tag` varchar(20) NOT NULL default '',
  `trainingDay2_halle` varchar(100) NOT NULL default '',
  `trainingDay2_fromHour` char(2) NOT NULL default '',
  `trainingDay2_fromMinutes` char(2) NOT NULL default '',
  `trainingDay2_toHour` char(2) NOT NULL default '',
  `trainingDay2_toMinutes` char(2) NOT NULL default '',
  `coach1_vorname` varchar(20) NOT NULL default '',
  `coach1_nachname` varchar(20) NOT NULL default '',
  `coach1_email` varchar(50) NOT NULL default '',
  `coach1_phone` varchar(20) NOT NULL default '',
  `coach2_vorname` varchar(20) NOT NULL default '',
  `coach2_nachname` varchar(20) NOT NULL default '',
  `coach2_email` varchar(50) NOT NULL default '',
  `coach2_phone` varchar(20) NOT NULL default '',
  PRIMARY KEY  (`teamid`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=14 ;

